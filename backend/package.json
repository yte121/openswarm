{
  "name": "claude-flow-dashboard-backend",
  "version": "2.0.0-alpha.84",
  "description": "Claude-Flow Dashboard Backend API Server - Enhanced Production Ready",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js --ignore logs/ --ignore data/",
    "test": "jest --runInBand",
    "test:watch": "jest --watch --runInBand",
    "lint": "eslint . --ext .js",
    "lint:fix": "eslint . --ext .js --fix",
    "format": "prettier --write .",
    "logs": "tail -f logs/backend.log",
    "logs:error": "tail -f logs/error.log",
    "health": "curl -f http://localhost:8001/api/health",
    "build": "echo 'No build step required for backend'",
    "clean": "rm -rf logs/* data/temp/*",
    "migrate": "node scripts/migrate.js",
    "seed": "node scripts/seed.js"
  },
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "ws": "^8.14.2",
    "helmet": "^7.1.0",
    "morgan": "^1.10.0",
    "compression": "^1.7.4",
    "dotenv": "^16.3.1",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "winston": "^3.11.0",
    "winston-daily-rotate-file": "^4.7.1",
    "node-cache": "^5.1.2",
    "uuid": "^9.0.1",
    "joi": "^17.11.0",
    "mongoose": "^8.0.3",
    "axios": "^1.6.2",
    "crypto": "^1.0.1",
    "fs-extra": "^11.2.0",
    "path": "^0.12.7",
    "cluster": "^0.7.7"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "jest": "^29.7.0",
    "supertest": "^6.3.3",
    "eslint": "^8.54.0",
    "prettier": "^3.1.0",
    "@types/node": "^20.10.5"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "keywords": [
    "claude-flow",
    "ai-orchestration",
    "dashboard",
    "api",
    "openrouter",
    "swarm-intelligence", 
    "enterprise",
    "production-ready"
  ],
  "author": "Claude-Flow Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ruvnet/claude-flow.git"
  }
}